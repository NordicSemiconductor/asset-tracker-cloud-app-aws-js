{
  "name": "@nordicsemiconductor/asset-tracker-cloud-app-aws",
  "version": "0.0.0-development",
  "description": "The nRF Asset Tracker Web Application for AWS is a reference single-page application (SPA) developed using TypeScript.",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "node --max_old_space_size=8192 --openssl-legacy-provider ./node_modules/vite/bin/vite.js build --emptyOutDir",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "node --experimental-vm-modules ./node_modules/jest/bin/jest.js --passWithNoTests",
    "test:e2e": "npx tsx e2e-tests/setup/setup.ts && npx playwright test e2e-tests/unauthenticated && npx playwright test e2e-tests/authenticated && npx playwright test e2e-tests/clean-up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js#readme",
  "keywords": [
    "nrf-asset-tracker",
    "iot",
    "web-app",
    "aws",
    "spa",
    "react",
    "vite"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@amcharts/amcharts5": "5.3.7",
    "@aws-amplify/ui-react": "4.4.1",
    "@aws-sdk/client-dynamodb": "3.296.0",
    "@aws-sdk/client-iot": "3.296.0",
    "@aws-sdk/client-iot-data-plane": "3.296.0",
    "@aws-sdk/client-s3": "3.296.0",
    "@aws-sdk/util-dynamodb": "3.296.0",
    "@nordicsemiconductor/asset-tracker-cloud-docs": "28.4.0",
    "@nordicsemiconductor/cell-geolocation-helpers": "6.0.0",
    "@nordicsemiconductor/from-env": "2.0.0",
    "@nordicsemiconductor/rsrp-bar": "6.0.0",
    "@nordicsemiconductor/timestream-helpers": "5.0.0",
    "@sentry/react": "7.44.2",
    "@sentry/tracing": "7.44.2",
    "@sinclair/typebox": "0.26.0",
    "ajv": "8.12.0",
    "aws-amplify": "5.0.21",
    "bootstrap": "5.2.3",
    "date-fns": "2.29.3",
    "e118-iin-list": "3.0.0",
    "fast-deep-equal": "3.1.3",
    "feather-icons": "4.29.0",
    "inter-ui": "3.19.3",
    "intro.js": "6.0.0",
    "mcc-mnc-list": "1.1.10",
    "nanoid": "4.0.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "4.2.1",
    "react-router-dom": "6.9.0",
    "semver": "7.3.8"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.296.0",
    "@aws-sdk/client-cognito-identity-provider": "3.296.0",
    "@aws-sdk/client-ssm": "3.296.0",
    "@aws-sdk/client-timestream-write": "3.296.0",
    "@aws-sdk/types": "3.296.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "12.0.9",
    "@nordicsemiconductor/object-to-env": "5.0.0",
    "@nordicsemiconductor/random-words": "7.0.0",
    "@playwright/test": "1.32.0",
    "@swc/core": "1.3.42",
    "@swc/jest": "0.2.24",
    "@types/chance": "1.1.3",
    "@types/feather-icons": "4.29.1",
    "@types/intro.js": "5.1.1",
    "@types/jest": "29.5.0",
    "@types/leaflet": "1.9.3",
    "@types/react": "18.0.28",
    "@types/react-dom": "18.0.11",
    "@types/semver": "7.3.13",
    "@vitejs/plugin-react": "3.1.0",
    "chance": "1.1.11",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-jsx-a11y": "6.7.1",
    "eslint-plugin-no-restricted-imports": "0.0.0",
    "handlebars": "4.7.7",
    "id128": "1.6.6",
    "identity-obj-proxy": "3.0.0",
    "isolate-react": "2.4.2",
    "jest": "29.5.0",
    "jest-environment-jsdom": "29.5.0",
    "tsx": "3.12.6",
    "typescript": "5.0.2",
    "vite": "4.2.1"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "jest": {
    "testRegex": ".+\\.spec\\.tsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e-tests/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^utils\\/(.*)\\.js$": "<rootDir>/src/utils/$1",
      "^hooks\\/(.*)\\.js$": "<rootDir>/src/hooks/$1",
      "^components\\/(.*)\\.js$": "<rootDir>/src/components/$1",
      "^asset\\/(.*)\\.js$": "<rootDir>/src/asset/$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.js"
    ]
  }
}
