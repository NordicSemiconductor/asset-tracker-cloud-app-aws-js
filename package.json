{
  "name": "@nordicsemiconductor/asset-tracker-cloud-app-aws",
  "version": "0.0.0-development",
  "description": "The nRF Asset Tracker Web Application for AWS is a reference single-page application (SPA) developed using TypeScript.",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "vite build --emptyOutDir",
    "preview": "vite preview",
    "prepare": "husky install",
    "test": "jest",
    "test:e2e": "tsc -p e2e-tests/tsconfig.json && npx playwright test -c e2e-tests-out"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js#readme",
  "keywords": [
    "nrf-asset-tracker",
    "iot",
    "web-app",
    "aws",
    "spa",
    "react",
    "vite"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@aws-amplify/ui-react": "2.1.10",
    "@aws-sdk/client-iot": "3.48.0",
    "@nordicsemiconductor/from-env": "1.0.0",
    "aws-amplify": "4.3.12",
    "bootstrap": "5.1.3",
    "feather-icons": "4.28.0",
    "inter-ui": "3.19.3",
    "intro.js": "4.3.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-leaflet": "3.2.5",
    "react-router-dom": "6.2.1"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.48.0",
    "@aws-sdk/client-cognito-identity-provider": "3.48.0",
    "@aws-sdk/client-ssm": "3.48.0",
    "@nordicsemiconductor/asset-tracker-cloud-code-style": "11.0.16",
    "@nordicsemiconductor/object-to-env": "3.0.0",
    "@playwright/test": "1.18.0",
    "@types/chance": "1.1.3",
    "@types/feather-icons": "4.7.0",
    "@types/intro.js": "3.0.2",
    "@types/jest": "27.4.0",
    "@types/react": "17.0.38",
    "@types/react-dom": "17.0.11",
    "@vitejs/plugin-react": "1.1.4",
    "chance": "1.1.8",
    "eslint-config-react-app": "7.0.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-no-restricted-imports": "0.0.0",
    "id128": "1.6.6",
    "jest": "27.4.7",
    "ts-jest": "27.1.3",
    "vite": "2.7.13",
    "vite-plugin-html": "2.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write",
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=16",
    "npm": ">=8"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      "@semantic-release/github"
    ]
  },
  "prettier": "@nordicsemiconductor/asset-tracker-cloud-code-style/.prettierrc",
  "jest": {
    "preset": "ts-jest",
    "testRegex": ".+\\.spec\\.ts$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e-tests/"
    ],
    "moduleNameMapper": {
      "^util/(.*)$": "<rootDir>/src/util/$1"
    }
  }
}
