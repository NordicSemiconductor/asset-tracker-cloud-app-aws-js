{
  "name": "@nordicsemiconductor/asset-tracker-cloud-app-aws",
  "version": "0.0.0-development",
  "description": "The nRF Asset Tracker Web Application for AWS is a reference single-page application (SPA) developed using TypeScript.",
  "type": "module",
  "scripts": {
    "start": "vite",
    "build": "node --max_old_space_size=8192 --openssl-legacy-provider ./node_modules/vite/bin/vite.js build --emptyOutDir",
    "preview": "vite preview",
    "prepare": "husky",
    "test": "node --experimental-vm-modules --no-warnings ./node_modules/jest/bin/jest.js --passWithNoTests",
    "test:e2e": "npx tsx e2e-tests/setup/setup.ts && npx playwright test e2e-tests/unauthenticated && npx playwright test e2e-tests/authenticated && npx playwright test e2e-tests/clean-up"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js.git"
  },
  "bugs": {
    "url": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js/issues"
  },
  "homepage": "https://github.com/NordicSemiconductor/asset-tracker-cloud-app-aws-js#readme",
  "keywords": [
    "nrf-asset-tracker",
    "iot",
    "web-app",
    "aws",
    "spa",
    "react",
    "vite"
  ],
  "author": "Nordic Semiconductor ASA | nordicsemi.no",
  "license": "BSD-3-Clause",
  "dependencies": {
    "@amcharts/amcharts5": "5.8.2",
    "@aws-amplify/core": "6.0.12",
    "@aws-amplify/ui": "6.0.10",
    "@aws-amplify/ui-react": "6.1.4",
    "@aws-sdk/client-dynamodb": "3.515.0",
    "@aws-sdk/client-iot": "3.515.0",
    "@aws-sdk/client-iot-data-plane": "3.515.0",
    "@aws-sdk/client-s3": "3.515.0",
    "@nordicsemiconductor/asset-tracker-cloud-docs": "36.0.2",
    "@nordicsemiconductor/cell-geolocation-helpers": "6.0.0",
    "@nordicsemiconductor/from-env": "3.0.0",
    "@nordicsemiconductor/rsrp-bar": "8.0.1",
    "@nordicsemiconductor/timestream-helpers": "6.0.1",
    "@sentry/react": "7.101.1",
    "@sentry/tracing": "7.101.1",
    "@sinclair/typebox": "0.32.9",
    "ajv": "8.12.0",
    "aws-amplify": "6.0.12",
    "bootstrap": "5.3.2",
    "date-fns": "3.3.1",
    "e118-iin-list": "4.1.2",
    "fast-deep-equal": "3.1.3",
    "feather-icons": "4.29.1",
    "inter-ui": "4.0.2",
    "intro.js": "7.2.0",
    "mcc-mnc-list": "1.1.11",
    "nanoid": "5.0.5",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-leaflet": "4.2.1",
    "react-router-dom": "6.22.0",
    "semver": "7.6.0"
  },
  "devDependencies": {
    "@aws-sdk/client-cloudformation": "3.515.0",
    "@aws-sdk/client-cognito-identity-provider": "3.515.0",
    "@aws-sdk/client-ssm": "3.515.0",
    "@aws-sdk/client-timestream-write": "3.515.0",
    "@aws-sdk/types": "3.515.0",
    "@aws-sdk/util-dynamodb": "3.515.0",
    "@bifravst/prettier-config": "1.0.0",
    "@commitlint/config-conventional": "18.6.1",
    "@nordicsemiconductor/object-to-env": "7.0.7",
    "@nordicsemiconductor/random-words": "8.0.0",
    "@playwright/test": "1.41.2",
    "@swc/core": "1.4.2",
    "@swc/jest": "0.2.36",
    "@types/chance": "1.1.6",
    "@types/feather-icons": "4.29.4",
    "@types/intro.js": "5.1.5",
    "@types/jest": "29.5.12",
    "@types/leaflet": "1.9.8",
    "@types/react": "18.2.57",
    "@types/react-dom": "18.2.19",
    "@types/semver": "7.5.7",
    "@typescript-eslint/eslint-plugin": "7.0.1",
    "@typescript-eslint/parser": "7.0.1",
    "@vitejs/plugin-react": "4.2.1",
    "chance": "1.1.11",
    "eslint-config-prettier": "9.1.0",
    "eslint-config-react-app": "7.0.1",
    "eslint-plugin-jsx-a11y": "6.8.0",
    "eslint-plugin-no-restricted-imports": "0.0.0",
    "eslint-plugin-prefer-arrow": "1.2.3",
    "eslint-plugin-unicorn": "51.0.1",
    "handlebars": "4.7.8",
    "husky": "9.0.11",
    "id128": "1.6.6",
    "identity-obj-proxy": "3.0.0",
    "isolate-react": "2.4.7",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "tsx": "4.7.1",
    "typescript": "5.3.3",
    "vite": "5.1.2"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "prettier --write"
    ],
    "**/*.{ts,tsx}": [
      "eslint --ext .js,.ts,.jsx,.tsx"
    ],
    "*.{md,json,yaml,yml,html}": [
      "prettier --write"
    ]
  },
  "engines": {
    "node": ">=20",
    "npm": ">=9"
  },
  "release": {
    "branches": [
      "saga"
    ],
    "remoteTags": true,
    "plugins": [
      "@semantic-release/commit-analyzer",
      "@semantic-release/release-notes-generator",
      [
        "@semantic-release/github",
        {
          "successComment": false,
          "failTitle": false
        }
      ]
    ]
  },
  "prettier": "@bifravst/prettier-config",
  "jest": {
    "testRegex": ".+\\.spec\\.tsx?$",
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/e2e-tests/"
    ],
    "moduleNameMapper": {
      "\\.(css|less|scss|sass)$": "identity-obj-proxy",
      "^utils\\/(.*)\\.js$": "<rootDir>/src/utils/$1",
      "^hooks\\/(.*)\\.js$": "<rootDir>/src/hooks/$1",
      "^components\\/(.*)\\.js$": "<rootDir>/src/components/$1",
      "^asset\\/(.*)\\.js$": "<rootDir>/src/asset/$1"
    },
    "transform": {
      "^.+\\.(t|j)sx?$": "@swc/jest"
    },
    "extensionsToTreatAsEsm": [
      ".ts",
      ".tsx"
    ],
    "setupFilesAfterEnv": [
      "<rootDir>/setup-jest.js"
    ]
  }
}
